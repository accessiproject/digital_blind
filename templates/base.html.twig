            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a href="{{ path('home_presentation') }}">Pr√©sentation de l'entreprise</a>
                    </li>
                    <li class="nav-item active">
                        <a href="{{ path('contact_index') }}">Contact</a>
                    </li>
                    <li class="nav-item active">
                        <a href="{{ path('home_admin') }}">Admin</a>
                    </li>
                </ul>
            </div>
        </nav>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
